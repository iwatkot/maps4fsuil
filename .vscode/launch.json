{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Streamlit: webui",
      "type": "debugpy",
      "request": "launch",
      "module": "streamlit",
      "args": ["run", "./maps4fsui/ui.py"],
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "env": {
        "PYTHONPATH": "${workspaceFolder}:${PYTHONPATH}",
        "LOG_LEVEL": "DEBUG"
      }
    }
  ]
}
